{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build user-auth",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/user-auth/GraphTutorial/GraphTutorial.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish user-auth",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/user-auth/GraphTutorial/GraphTutorial.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch user-auth",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "--project",
                "${workspaceFolder}/user-auth/GraphTutorial/GraphTutorial.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build app-auth",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/app-auth/GraphAppOnlyTutorial/GraphAppOnlyTutorial.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish app-auth",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/app-auth/GraphAppOnlyTutorial/GraphAppOnlyTutorial.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch app-auth",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "--project",
                "${workspaceFolder}/app-auth/GraphAppOnlyTutorial/GraphAppOnlyTutorial.csproj"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}